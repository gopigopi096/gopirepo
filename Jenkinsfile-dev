node('dev_1') {
  
  stages {
    stage("checkout sourcecode") {
        checkout scm
    }
    
    stage("node modules install") {
        steps {
          sh 'npm install'
        }
    }
    
    stage('Build') {
        milestone()
        sh 'ng build --prod --aot --sm --progress=false'
    }
  }

}
